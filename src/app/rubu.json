    "Texte_Rubu": "اقْتَرَبَ لِلنَّاسِ حِسَابُهُمْ"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "أَوَلَمْ يَرَ الَّذِينَ كَفَرُوا أَنَّ السَّمَواتِ"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "وَلَقَدْ آتَيْنَا إِبْرَاهِيمَ رُشْدَهُ"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "وذا النون إذ ذهب مغاضبا"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "يَا أَيُّهَا النَّاسُ اتَّقُوا رَبَّكُمْ"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "هَذَانِ خَصْمَانِ اخْتَصَمُوا فِي رَبِّهِمْ"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "إِنَّ اللَّهَ يُدَافِعُ عَنِ الَّذِينَ آمَنُوا"
  },
  {
    "Juz": 17,
    "Texte_Rubu": "ألم تر أن الله أنزل من السماء"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "قَدْ أَفْلَحَ الْمُؤْمِنُونَ"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "قال رب انصرني بما كذبون"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "ولورحمناهم وكشفنا"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "سُورَةٌ أَنْزَلْنَاهَا وَفَرَضْنَاهَا"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "يَا أَيُّهَا الَّذِينَ آمَنُوا لَا تَتَّبِعُوا"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "اللَّهُ نُورُ السَّمَواتِ وَالْأَرْضِ"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "وَأَقْسَمُوا بِاللَّهِ جَهْدَ أَيْمَانِهِمْ"
  },
  {
    "Juz": 18,
    "Texte_Rubu": "تَبَارَكَ الَّذِي نَزَّلَ الْفُرْقَانَ"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "وَقَالَ الَّذِينَ لَا يَرْجُونَ لِقَاءَنَا"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "ألم تر إلى ربك كيف مدَّ الظل"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "طسم تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "قالوا لاضير إناإلى ربنا منقلبون"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "قالواأنومن لك واتبعك الارذلون"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "أوفواالكيل ولاتكونوا من المخسرين"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "طس تِلْكَ آيَاتُ الْقُرْآنِ وَكِتَابٍ مُبِينٍ"
  },
  {
    "Juz": 19,
    "Texte_Rubu": "قال سننظر أصدقت أم كنت"
  },
  {
    "Juz": 20,
    "Texte_Rubu": "فماكان جواب قومه"
  },
  {
    "Juz": 20,
    "Texte_Rubu": "وَإِذَا وَقَعَ الْقَوْلُ عَلَيْهِمْ أَخْرَجْنَا"
